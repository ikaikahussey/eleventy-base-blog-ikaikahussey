<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">
    <link rel="stylesheet" href="https://cdn.skypack.dev/-/sakura.css@v1.3.1-VmH2VZPA3IgYndF0s0Cx/dist=es2020,mode=raw/css/sakura.css" type="text/css">
	<link rel="stylesheet" href="/css/index.css" type="text/css">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
  </head>
  <body class="">
	<nav class="">
				<h1 class="">
			<a href="{{ '/' | url }}">
				{{ metadata.title }}
			</a>
		</h1>
		<ul>
		{%- for entry in collections.all | eleventyNavigation %}
		<li>
			<a class="" aria-current="page" href="{{ entry.url | url }}">{{ entry.title }}</a>
		</li>
		{%- endfor %}
		</ul>
	</nav>
    <main class="row{% if templateClass %} {{ templateClass }}{% endif %}">
	    <section class="column">
			{{ content | safe }}
	    </section>
		<section class="column" id="sidebar">
			<h2>links</h2>
			<ul>
				<li>Faith Action</li>
				<li>Local 5</li>
				<li></li>
			</ul>
		</section>
    </main>
    <footer class="">
    </footer>
    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
